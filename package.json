{
  "name": "@hidden-umbrella/atsui",
  "version": "1.0.1",
  "main": "dist/src/index",
  "author": "ArtieFuzzz#8298",
  "license": "MIT",
  "private": false,
  "description": "Search steam games and get the info about it",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "build": "yarn clean && yarn build:ts",
    "build:ts": "tsc -p ./",
    "clean": "rimraf ./dist && mkdir dist",
    "doc:gen": "typedoc",
    "prepare": "husky install .github/husky",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@hidden-umbrella/eslint-config": "^1.0.2",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.10.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typedoc": "^0.21.9",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=15",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hidden-umbrella/tsuki.git"
  },
  "bugs": {
    "url": "https://github.com/hidden-umbrella/tsuki/issues"
  },
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "keywords": [
    "api",
    "steam"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@aero/centra": "^1.0.7"
  }
}
